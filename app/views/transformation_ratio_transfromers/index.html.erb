<p id="notice"><%= notice %></p>

<h1>Transformation Ratio Transfromers</h1>

<table>
  <thead>
    <tr>
      <th>Outlet</th>
      <th>Relationship</th>
      <th>Step a</th>
      <th>Step b</th>
      <th>Step c</th>
      <th>Difference</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transformation_ratio_transfromers.each do |transformation_ratio_transfromer| %>
      <tr>
        <td><%= transformation_ratio_transfromer.outlet %></td>
        <td><%= transformation_ratio_transfromer.relationship %></td>
        <td><%= transformation_ratio_transfromer.step_a %></td>
        <td><%= transformation_ratio_transfromer.step_b %></td>
        <td><%= transformation_ratio_transfromer.step_c %></td>
        <td><%= transformation_ratio_transfromer.difference %></td>
        <td><%= link_to 'Show', transformation_ratio_transfromer %></td>
        <td><%= link_to 'Edit', edit_transformation_ratio_transfromer_path(transformation_ratio_transfromer) %></td>
        <td><%= link_to 'Destroy', transformation_ratio_transfromer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

